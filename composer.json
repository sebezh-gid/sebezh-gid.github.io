{
    "name": "umonkey/sebezh-gid",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["website"],
    "homepage": "https://sebezh-gid.ru/",
    "license": "MIT",
    "authors": [
        {
            "name": "Justin Forest",
            "email": "hex@umonkey.net",
            "homepage": "https://umonkey.net/"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "php-di/slim-bridge": "^3.0",
        "slim/psr7": "^1.2",
        "slim/slim": "^4.0",
        "psr/simple-cache": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.63",
        "phpstan/phpstan-strict-rules": "^0.12.7",
        "squizlabs/php_codesniffer": "^3.5"
    }
}
